{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "arrow-body-style": "off",
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }],
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "consistent-return": "off",
    "no-nested-ternary": "off"
  }
}
